{
  "manifest_version": 2,

  "name": "Probtn exist checker",
  "description": "This extension check is probtn exist at current page",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": { "scripts": ["background.js"] },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
